extends layout

block content
  .container
    ul
      each path in dirs
        li.list-group-item
          a(href=path.name + '/')= path.name
    ul
      each path in files
        a(href='/' + path.path + '/' + path.name)
          img.img-thumbnail(src='/' + path.path + '/' + path.name, width=128, height=128)
